<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="admin.AdminController">
   <children>
      <TabPane layoutX="7.0" layoutY="6.0" prefHeight="720.0" prefWidth="1280.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="Users">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <HBox layoutX="331.0" prefHeight="691.0" prefWidth="948.0">
                           <children>
                              <TableView fx:id="userTable" prefHeight="691.0" prefWidth="945.0">
                                <columns>
                                  <TableColumn fx:id="columnName" prefWidth="180.0" text="Användare" />
                                  <TableColumn fx:id="columnPassword" prefWidth="180.0" text="Lösenord" />
                                  <TableColumn fx:id="columnRole" prefWidth="180.0" text="Roll" />
                                </columns>
                              </TableView>
                           </children>
                        </HBox>
                        <VBox layoutX="8.0" layoutY="3.0" prefHeight="679.0" prefWidth="319.0">
                           <children>
                              <Label prefHeight="25.0" prefWidth="323.0" text="Lägg till ny användare" textFill="#3d2ab9">
                                 <padding>
                                    <Insets left="50.0" />
                                 </padding>
                                 <font>
                                    <Font name="Elephant" size="14.0" />
                                 </font>
                              </Label>
                              <TextField fx:id="firstAndLastName" maxWidth="200" promptText="Förnamn efternamn">
                                 <VBox.margin>
                                    <Insets left="30.0" top="10.0" />
                                 </VBox.margin></TextField>
                               <TextField fx:id="password" maxWidth="200" promptText="Lösenord">
                                   <VBox.margin>
                                       <Insets left="30.0" top="10.0" />
                                   </VBox.margin></TextField>
                              <Button mnemonicParsing="false" text="Lägg tll användare">
                                 <VBox.margin>
                                    <Insets left="30.0" top="10.0" />
                                 </VBox.margin>
                              </Button>
                              <Button mnemonicParsing="false" onAction="#loadUserData" text="Ladda användare">
                                 <VBox.margin>
                                    <Insets left="30.0" top="10.0" />
                                 </VBox.margin>
                              </Button>
                              <Button mnemonicParsing="false" text="Rensa formulär">
                                 <VBox.margin>
                                    <Insets left="30.0" top="10.0" />
                                 </VBox.margin>
                              </Button>
                           </children>
                        </VBox>
                     </children></AnchorPane>
            </content>
          </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
